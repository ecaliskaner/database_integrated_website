<include>
  <extension name="netgsm-inbound">
    <condition field="network_addr" expression="^.*$">
      <!-- Forward all inbound calls to Drachtio -->
      <action application="set" data="sip_h_X-Forwarded-By=freeswitch"/>
      <action application="bridge" data="sip:127.0.0.1:5062"/>
    </condition>
  </extension>
</include>


